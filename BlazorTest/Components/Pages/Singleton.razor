@page "/Singleton"
@inject ISingletonService _singletonService;

<PageTitle>Home</PageTitle>

<h1>@_guid1</h1>
<h1>@_guid2</h1>

@code {
    private string? _guid1;
    private string? _guid2;

    protected override void OnInitialized()
    {
        _guid1 = _singletonService.GetGuid().ToString();
        _guid2 = _singletonService.GetGuid().ToString();
    }

}
